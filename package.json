    "bootstrap": "^5.1.3",
    "browser-sync": "^2.27.9",
    "browser-sync-webpack-plugin": "^2.3.0",
    "laravel-mix": "^6.0.43",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0"
  }

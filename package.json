{
  "name": "mix",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.mix.js",
  "scripts": {
    "dev": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production",
    "dev:main": "BUILD_TARGET=main mix",
    "dev:custom-editor": "BUILD_TARGET=custom-editor mix",
    "dev:block-editor": "BUILD_TARGET=block-editor mix",
    "watch:main": "BUILD_TARGET=main mix watch",
    "watch:custom-editor": "BUILD_TARGET=custom-editor mix watch",
    "watch:block-editor": "BUILD_TARGET=block-editor mix watch",
    "prod:main": "BUILD_TARGET=main mix --production",
    "prod:custom-editor": "BUILD_TARGET=custom-editor mix --production",
    "prod:block-editor": "BUILD_TARGET=block-editor mix --production"
  },
